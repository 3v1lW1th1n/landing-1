extends ../layout

block content
    section.banner
        .animated.bounceInLeft.fast
            .container
                .row
                    .banner__content
                        h1 JSDADDY
                        p Reach your business goals with awesome apps in a cost-effective and timely manner, using lean philosophy principles.
                            principles.
                        a(href=".popup-form" class="btn contact_us_btn") Contact Us
    section.services#services
        .container
            .row
                .short-header SERVICES
                h3 WEB MOBILE SOLUTIONS. ENTERPRISE APPS PORTALS
                .sub-header Full-cycle servicing that covers every
            .row.services-items-wrapper
                .service.col-md-4
                    .item.animated.wow.fadeInLeft.faster
                        a(href='#')
                            .item__icon
                                img(src="/img/web.svg", alt="web")
                            h5 Advanced WEB
                            p  Productive, engaging
                .service.col-md-4
                    .item.animated.wow.fadeInUp.faster
                        a(href='#')
                            .item__icon
                                img(src="img/smartphone.svg", alt="mobile app")
                            h5 Advanced APP
                            p  Productive, engaging and reliable solutions
                .service.col-md-4
                    .item.animated.wow.fadeInRight.faster
                        a(href='#')
                            .item__icon
                                img(src="/img/cloud.svg", alt="cloud")
                            h5 Advanced CLOUD
                            p  Productive, engaging and reliable solutions
    section.dev-process#dev-process
        .container
            .row
                .short-header Process
                h3 Development Process
                .sub-header Do you have an idea?
            .row
                .step-wrapper
                    .left-col
                        .step.planing.animated.wow.fadeInLeft.faster
                            .text-wrap
                                h6 PLANNING
                                p Perfect
                            .ico-wrap
                                span.icon-planning
                        .step.analysis.animated.wow.fadeInLeft.faster
                            .text-wrap
                                h6 ANALYSIS
                                p Analysis
                            .ico-wrap
                                span.icon-analysis
                        .step.stack.animated.wow.fadeInLeft.faster
                            .text-wrap
                                h6 Design
                                p Setting a standard
                            .ico-wrap
                                span.icon-design
                    .center-col
                        img(src='img/construction_project_plan_building_architect_design_develop-83_icon-icons.com_60220.png', alt='asd')
                    .right-col
                        .step.architecture.animated.wow.fadeInRight.faster
                            .ico-wrap
                                span.icon-implementation
                            .text-wrap
                                h6 IMPLEMENTATION
                                p Developing
                        .step.test.animated.wow.fadeInRight.faster
                            .ico-wrap
                                span.icon-test
                            .text-wrap
                                h6 Testing
                                p Checking
                        .step.deployment.animated.wow.fadeInRight.faster
                            .ico-wrap
                                span.icon-MAINTENANCE
                            .text-wrap
                                h6 MAINTENANCE
                                p Changing
