    "@types/body-parser": "1.17.0",
    "@types/config": "0.0.34",
    "@types/cors": "2.8.4",
    "@types/express": "4.11.1",
    "@types/jquery": "^3.3.10",
    "@types/mongoose": "5.0.12",
    "@types/mustache": "0.8.30",
    "@types/node": "^10.0.4",
    "@types/nodemailer": "4.6.0",
    "@types/compression": "^1.0.1",
    "awesome-typescript-loader": "5.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "mongoose": "5.0.17",
