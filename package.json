{
  "name": "landing",
  "version": "1.0.0",
  "description": "#### Installation ``` ## Install dart using brew to your local machine brew tap dart-lang/dart brew install dart --devel ```",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "nodemon": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "build:css && build:js && pm2 start src/main.ts",
    "lint": "tslint 'src/**/*.ts'",
    "start:dev": "npm-run-all --parallel build:css:w build:js:w nodemon",
    "build:css:w": "node-sass -w --include-path scss web/sass/index.sass public/css/site.min.css --output-style compressed",
    "build:js:w": "webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JsDaddy/landing.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JsDaddy/landing/issues"
  },
  "homepage": "https://github.com/JsDaddy/landing#readme",
  "dependencies": {
    "axios": "0.18.0",
    "config": "1.30.0",
    "express": "4.16.3",
    "jquery": "3.3.1",
    "pug": "2.0.3"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/config": "0.0.34",
    "@types/express": "4.11.1",
    "@types/mongoose": "5.0.12",
    "@types/node": "10.0.4",
    "awesome-typescript-loader": "5.0.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.4",
    "pre-commit": "1.2.2",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "typescript": "2.8.3",
    "webpack": "4.8.0",
    "webpack-cli": "2.1.3",
    "mongoose": "5.0.17",
    "npm-run-all": "4.1.3"
  },
  "pre-commit": [
    "lint"
  ]
}
